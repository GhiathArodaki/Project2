   System.out.println("Is that a Yes or a No?");
   Scanner answer = new Scanner(System.in);
   answer_zodiac = answer.nextLine();
   switch(answer_zodiac) {
     case "Yes":
     System.out.println("You're loyal");
     break;
     case "No":
     System.out.println("Let me try again.\n");
     System.out.println("Pick a card!\n");
     for (int c : cards) {
    System.out.println(c);
    }
    card = user_input.nextInt();
   }
